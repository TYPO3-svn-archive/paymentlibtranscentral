							<numIndex index="1">12</numIndex>
						</numIndex>
					</items>
					<eval>required,ccexpdatemonth</eval>
				</config>
			</cc_expdate_month>
			<cc_expdate_year type="array">
				<label>LLL:EXT:paymentlib_transcentral/locallang.php:fields_expirydateyear</label>
				<config type="array">
					<type>select</type>
					<items>
						<numIndex index="0">
							<numIndex index="0"></numIndex>
							<numIndex index="1">1</numIndex>
						</numIndex>
					</items>
					<itemsProcFunc>tx_paymentlibtranscentral_provider->itemProcFunc_ccexpdateyear_items</itemsProcFunc>
